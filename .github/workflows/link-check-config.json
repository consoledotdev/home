{
    "ignorePatterns": [
        {
            "pattern": "/.png$/"
        }
    ],
    "replacementPatterns": [
        {
            "pattern": "/&quot;$/",
            "replacement": ""
        },
        {
            "pattern": "/\"$/",
            "replacement": ""
        },
        {
            "pattern": "^/",
            "replacement": "https://console.dev/"
        }
    ],
    "retryOn429": true,
    "aliveStatusCodes": [
        200,
        206,
        403,
        999
    ]
}